/*Button*/
.login-button {
	FONT-FAMILY: Tahoma;
	font-size: 11px;
	font-weight: bold;
	color: #FFFFFF;
	position: relative;
	left: 0px;
}

#button span {
	display: block;
	float: left;
	background: url(images/buttonv1/btn_midd.png);
	background-repeat: repeat-x;
	height: 27px;
	text-decoration: none;
}

#button  b {
	display: block;
	float: left;
	background: url(images/buttonv1/btn_right.png);
	background-repeat: no-repeat;
	width: 5px;
	height: 27px;
	text-decoration: none;
}

#button a {
	line-height: 35px; /*equal to the height of the button*/
	cursor: hand;
	text-decoration: none;
}

#button em {
	display: block;
	float: left;
	background: url(images/buttonv1/btn_left.png);
	background-repeat: no-repeat;
	width: 5px;
	height: 27px;
	text-decoration: none;
}

.detailInfoFont {
	font-family: Arial, Helvetica, sans-serif;
	color: #000000;
	font-size: 12px;
}

.titleBand {
	font-family: Arial, Helvetica, sans-serif;
	background: #D7DF01;
	font-size: 14px;
}

.toolsHeadingFont {
	font-family: Arial, Helvetica, sans-serif;
	color: #8C6845;
	font-size: 16px;
	font-weight: bold;
	font-style: italic;
}



