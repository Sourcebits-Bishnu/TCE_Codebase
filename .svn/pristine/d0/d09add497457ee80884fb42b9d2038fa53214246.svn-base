/* Globallike Detailsbutton*/
.cancelButton span {
	background: url(images/buttonv2/button_left.png) no-repeat 0 0;
	display: block;
	line-height: 10px;
	padding: 5px 0 8px 10px;
	color: #fff;
	font-size: 12px;
}

.cancelButton {
	background: url(images/buttonv2/button_right.png) no-repeat top right;
	display: block;
	float: left;
	height: 23px;
	padding-right: 12px;
	text-decoration: none;
	font-size: 11px;
	margin: 0px 8px 0 0;
	text-shadow: 0 1px 1px #386c99, 0 1px 1px rgba(19, 92, 153, 0.35);
	cursor : pointer;
}

.cancelButton:hover span {
	background: url(images/buttonv2/button_left.png) no-repeat 0
		0;
	color: #fff;
}

.cancelButton:hover {
	background: url(images/buttonv2/button_right.png) no-repeat
		top right;
}

/* Globallike Detailsbutton*/

.divSeperator {
	background: url(images/portletv1/resoursebg.png) repeat-x left
		top #e7f4ff;
	border: 1px solid #cbddf0;
	float: left;
	font-size: 12px;
	color: #4c4c4c;
	padding: 5px;
	width: 892px;
	-moz-border-radius: 3px; /* firefox mozilla*/
	-webkit-border-radius: 3px; /*safari, chrome*/
	border-radius: 3px; /*opera*/
	margin: 0 0 5px 0;
}

span.checked{ background:url(images/checkboxbg.png) no-repeat left top !important; width:20px; height:15px; display:block; float:left; }
span.unchecked{ background:url(images/checkboxbg.png) no-repeat left -24px !important; width:20px; height:15px; display:block; float:left; }

